<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.fragment.GatheringDetailsFragment">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="116dp"
                android:layout_marginTop="40dp"
                android:text="@string/date"
                app:layout_constraintStart_toEndOf="@+id/gatheringDetails_time"
                app:layout_constraintTop_toBottomOf="@+id/gatheringDetails_sport" />

            <TextView
                android:id="@+id/gatheringDetails_usersTextView"
                android:layout_width="14dp"
                android:layout_height="23dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="44dp"
                android:text="0"
                android:visibility="visible"
                app:layout_constraintStart_toEndOf="@+id/imageButton2"
                app:layout_constraintTop_toBottomOf="@+id/gatheringDetails_address" />

            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="76dp"
                android:layout_marginTop="44dp"
                android:background="@android:color/transparent"
                android:visibility="visible"
                app:layout_constraintStart_toEndOf="@+id/gatheringDetails_sport"
                app:layout_constraintTop_toBottomOf="@+id/gatheringDetails_address"
                app:srcCompat="@drawable/ic_baseline_person_24" />

            <com.google.android.gms.maps.MapView
                android:id="@+id/gatheringDetails_miniMap"
                android:layout_width="match_parent"
                android:layout_height="287dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

            <Button
                android:id="@+id/gatheringDetails_subscribe_button"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="@string/subscribe"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/gatheringDetails_miniMap" />

            <TextView
                android:id="@+id/gatheringDetails_sport_textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="44dp"
                android:text="@string/sport"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.111"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/gatheringDetails_address" />

            <TextView
                android:id="@+id/gatheringDetails_sport"
                android:layout_width="96dp"
                android:layout_height="22dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="44dp"
                app:layout_constraintStart_toEndOf="@+id/gatheringDetails_sport_textView"
                app:layout_constraintTop_toBottomOf="@+id/gatheringDetails_address" />

            <TextView
                android:id="@+id/gatheringDetails_address_textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="36dp"
                android:text="@string/address"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/gatheringDetails_subscribe_button" />

            <TextView
                android:id="@+id/gatheringDetails_address"
                android:layout_width="75dp"
                android:layout_height="19dp"
                android:layout_marginTop="12dp"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/gatheringDetails_address_textView" />

            <TextView
                android:id="@+id/gatheringDetails_time_textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="48dp"
                android:text="@string/time"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.111"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/gatheringDetails_sport" />

            <TextView
                android:id="@+id/gatheringDetails_time"
                android:layout_width="49dp"
                android:layout_height="21dp"
                android:layout_marginTop="48dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.01"
                app:layout_constraintStart_toEndOf="@+id/gatheringDetails_time_textView"
                app:layout_constraintTop_toBottomOf="@+id/gatheringDetails_sport" />

            <TextView
                android:id="@+id/gatheringDetails_coast_textView"
                android:layout_width="39dp"
                android:layout_height="25dp"
                android:layout_marginStart="240dp"
                android:layout_marginTop="16dp"
                android:text="@string/coast"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView1" />

            <TextView
                android:id="@+id/gatheringDetails_coast"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="20dp"
                android:text="100"
                app:layout_constraintStart_toEndOf="@+id/gatheringDetails_coast_textView"
                app:layout_constraintTop_toBottomOf="@+id/textView1" />

            <TextView
                android:id="@+id/gatheringDetails_date"
                android:layout_width="64dp"
                android:layout_height="22dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="36dp"
                app:layout_constraintStart_toEndOf="@+id/textView1"
                app:layout_constraintTop_toBottomOf="@+id/gatheringDetails_sport" />

            <TextView
                android:id="@+id/gatheringDetails_description_textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/gatheringDetails_coast_textView" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>